{
    "name": "my-site",
    "version": "1.0.0",
    "repository": "github.com:n06Ar/MySite.git",
    "author": "NobuhitoArai <nobht.arai@gmail.com>",
    "private": true,
    "scripts": {
        "dev": "parcel serve src/index.html",
        "build": "rm -rf docs && parcel build src/index.html -d docs",
        "format": "prettier --write **/*.{js,json,md,html}"
    },
    "prettier": {
        "singleQuote": true,
        "semi": false,
        "tabWidth": 4,
        "arrowParens": "avoid",
        "svelteBracketNewLine": true,
        "svelteStrictMode": true
    },
    "dependencies": {
        "parcel": "^1.12.4",
        "svelte": "^3.6.7",
        "svelte-router-spa": "^5.7.2",
        "uikit": "^3.5.7"
    },
    "devDependencies": {
        "@tsconfig/svelte": "^1.0.4",
        "node-sass": "^4.14.1",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-svelte": "^4.0.6",
        "prettier": "^2.0.5",
        "prettier-plugin-svelte": "^1.1.0",
        "sass": "^1.26.9",
        "svelte-awesome": "^2.3.0",
        "svelte-check": "^1.0.0",
        "svelte-preprocess": "^4.0.9",
        "svelte-spa-router": "^2.2.0",
        "typescript": "^3.9.7"
    },
    "browserslist": [
        "last 1 chrome versions"
    ]
}
